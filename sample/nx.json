{
  "npmScope": "sample",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "api": {
      "tags": []
    },
    "shared-auth": {
      "tags": []
    },
    "shared-ui-table": {
      "tags": ["shared", "ui"]
    },
    "tuition": {
      "tags": []
    },
    "tuition-e2e": {
      "tags": [],
      "implicitDependencies": ["tuition"]
    },
    "tuition-feature-lesson-plan": {
      "tags": ["tuition", "feature"]
    }
  }
}
